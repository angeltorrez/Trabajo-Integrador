{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/(.*)$",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "ACCESS_TOKEN_SECRET": "@jwt_access_secret",
    "REFRESH_TOKEN_SECRET": "@jwt_refresh_secret",
    "NODE_ENV": "production"
  }
}
